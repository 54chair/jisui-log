<section class="post-like-users-list">
  <div class="post-like-user-list-item">
    <h2>投稿にいいねしたユーザー一覧</h2>
  </div>
  <div class="user-list-group">
    <div class="row d-flex justify-content-center">
      <div class ="col-md-6">
        <ul class="list-group">
          <% @likes_users.each do |user| %>
            <li class="list-group-item">
              <%= link_to user.username, user_path(user) %>
            </li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</section>