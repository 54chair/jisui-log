<%= form_with(model: @post, url:"/posts/#{@post.id}/update", local: true) do |f| %>

  <%= f.label :title, "料理名" %>
  <%= f.text_field :title %>

  <%= f.label :content, "感想" %>
  <%= f.text_area :content %>
  
  <%= f.label :image, "料理の画像" %>
  <%= f.file_field :image, accept: "image/jpeg,image/gif,image/png" %>

  <%= f.submit "編集" %>
<% end %>